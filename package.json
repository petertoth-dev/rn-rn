{
  "name": "rn-rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "NODE_ENV=development react-native run-android",
    "android:build:release": "NODE_ENV=production react-native build-android --mode=release",
    "android:build:debug": "NODE_ENV=development cd android && ./gradlew assembleDebug",
    "android:build:release-apk": "NODE_ENV=production cd android && ./gradlew assembleRelease",
    "ios": "NODE_ENV=development react-native run-ios --scheme development",
    "ios:production": "NODE_ENV=production react-native run-ios --scheme production",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "./nuke.sh",
    "pod:install": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.80.1",
    "react": "19.1.0",
    "react-native": "0.80.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
